{
  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "ES2022",
    "moduleResolution": "Node",
    "target": "ES2021",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "incremental": true
  }
}
