{
  "compilerOptions": {
    "lib": ["ES2021", "DOM", "DOM.Iterable"],
    "module": "ES2022",
    "target": "ES2021",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node16",
    "exactOptionalPropertyTypes": true,
    "importsNotUsedAsValues": "error",
    "checkJs": true,
    "outDir": "dist"
  }
}
