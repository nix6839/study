{
  "name": "a_1_react-test-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier --write **/*.{ts,tsx,json,html,cjs,css}",
    "lint": "eslint **/*.{ts,tsx}",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui --api 5000",
    "type-check": "tsc"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/eslint": "^8.4.5",
    "@types/prettier": "^2.6.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^2.0.0",
    "@vitest/ui": "^0.19.1",
    "eslint": "^8.20.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-testing-library": "^5.5.1",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.19.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/plugin-syntax-flow",
        "@babel/plugin-transform-react-jsx"
      ]
    }
  }
}
